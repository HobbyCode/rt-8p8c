loadrt threads name1=test period1=1000000
loadrt siggen
loadrt pokeys timeout=500000

net X-vel siggen.0.cosine => pokeys.PEv2.AxisID[0].velocity-cmd
net Y-vel siggen.0.sine => pokeys.PEv2.AxisID[1].velocity-cmd
net Z-vel siggen.0.sawtooth => pokeys.PEv2.AxisID[2].velocity-cmd
net A-vel siggen.0.triangle => pokeys.PEv2.AxisID[3].velocity-cmd
setp pokeys.PEv2.AxisID[0].maxaccel 1
setp pokeys.PEv2.AxisID[0].maxvel 2
setp pokeys.PEv2.AxisID[0].scale 10000
setp pokeys.PEv2.AxisID[0].stepwidth 15
setp pokeys.PEv2.AxisID[0].dirhold 1
setp pokeys.PEv2.AxisID[0].dirsetup 1
setp pokeys.PEv2.AxisID[1].maxaccel 1
setp pokeys.PEv2.AxisID[1].maxvel 2
setp pokeys.PEv2.AxisID[1].scale 10000
setp pokeys.PEv2.AxisID[1].stepwidth 15
setp pokeys.PEv2.AxisID[1].dirhold 1
setp pokeys.PEv2.AxisID[1].dirsetup 1
setp pokeys.PEv2.AxisID[2].maxaccel 1
setp pokeys.PEv2.AxisID[2].maxvel 2
setp pokeys.PEv2.AxisID[2].scale 10000
setp pokeys.PEv2.AxisID[2].stepwidth 15
setp pokeys.PEv2.AxisID[2].dirhold 1
setp pokeys.PEv2.AxisID[2].dirsetup 1
setp pokeys.PEv2.AxisID[3].maxaccel 1
setp pokeys.PEv2.AxisID[3].maxvel 2
setp pokeys.PEv2.AxisID[3].scale 10000
setp pokeys.PEv2.AxisID[3].stepwidth 15
setp pokeys.PEv2.AxisID[3].dirhold 1
setp pokeys.PEv2.AxisID[3].dirsetup 1
setp pokeys.0.enable 1
setp pokeys.1.enable 1
setp pokeys.2.enable 1
setp pokeys.3.enable 1

addf pokeys.read test
addf pokeys.supervisor test
addf siggen.0.update test
addf pokeys.stepgen test
addf pokeys.write test
start

